{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "globals": {
    "BASE_URL": true,
    "API_URL": true,
    "SSR_PORT": true
  },
  "rules": {
    "function-paren-newline": ["error", "consistent"],
    "object-curly-newline": "off",
    "jsx-a11y/anchor-is-valid": ["error", { "specialLink": ["to"] }]
  }
}
