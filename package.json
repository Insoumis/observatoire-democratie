{
  "name": "observatoire-democratie",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "webpack-dev-server --hot --history-api-fallback --open --host 0.0.0.0",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "start:prod": "cross-env NODE_ENV=production node build/server.bundle.js",
    "clean": "rimraf build",
    "lint": "eslint ."
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.1",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "react-hot-loader": "^3.1.1",
    "rimraf": "^2.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "react-router-scroll": "^0.4.3",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2"
  }
}
